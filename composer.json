{
    "name": "cartalyst/stripe-laravel",
    "description": "Laravel 5.8 integration for the Cartalyst Stripe package.",
    "keywords": [
        "php",
        "cartalyst",
        "laravel",
        "stripe"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "5.8.*",
        "cartalyst/stripe": "~2.0"
    },
    "require-dev": {
        "cartalyst/php-cs-fixer-config": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Cartalyst\\Stripe\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cartalyst\\Stripe\\Laravel\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "component": "implementation",
        "branch-alias": {
            "dev-master": "10.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Cartalyst\\Stripe\\Laravel\\StripeServiceProvider"
            ],
            "aliases": {
                "Stripe": "Cartalyst\\Stripe\\Laravel\\Facades\\Stripe"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
